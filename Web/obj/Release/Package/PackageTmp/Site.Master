    <script type="text/javascript" charset="utf-8">
        // initialise plugins
        $(document).ready(function () {
            $(".datepicker").datepicker({
                format: 'dd/mm/yyyy',
                isRTL: false,
                autoclose: true,
                language: 'vi',

            });

            $(".datepicker").datepicker();


            $(".sidebar-menu li").eq(1).addClass("active");


            getCurrentMenu();
        });

        function getCurrentMenu() {
            var currentUrl = window.location.href;

            var agrList = currentUrl.split("?");
            if (agrList[1] != null)
                currentUrl = agrList[0];

            //if (currentUrl.indexOf("?") > -1) {
            //    alert(1);
            //}

            //console.log("abc", currentUrl);
            //console.log("agrList1", agrList[1]);

            var menuList = $(".sidebar-menu > li > ul > li");

            for (var i = 0; i < menuList.length; i++) {

                var menuLink = $(menuList[i]).children("a");
                if ($(menuLink).attr('href') == currentUrl) {
                    setActiveSubMenuTop($(menuList[i]));
                }
            }

            var subMenuList = $("#menu-top > ul > li > ul > li > ul > li");

            for (var i = 0; i < subMenuList.length; i++) {

                var menuLink = $(subMenuList[i]).children("a");
                var parentMenu = $(subMenuList[i]).parent().parent();

                if ($(menuLink).attr('href') == currentUrl) {
                    setActiveSubMenuTop(parentMenu);
                }
            }
        }

        function setActiveSubMenuTop(menuLi) {
            $(".sidebar-menu > li li").removeClass("current");
            menuLi.addClass("current");

            var parentMenuName = menuLi.parent().prev().children("span").html();
            setActiveMenuTopInstantly(parentMenuName);
        }

        function setActiveMenuTopInstantly(menuName) {

            $(".sidebar-menu > li").removeClass("active");
            //$(".sidebar-menu > li > ul").height(0);
            $(".sidebar-menu > li").each(function () {
                if ($(this).children("a").children("span").html() == menuName) {
                    $(this).addClass("active");
                    //var ulMenu = $(this).children("ul");
                    //var height = 0;
                    //ulMenu.children("li").each(function () {
                    //    height += $(this).height() + 1;
                    //});

                    //ulMenu.height(height);
                }
            });

        }


    </script>

