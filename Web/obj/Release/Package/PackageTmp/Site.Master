        $(document).ready(function () {
            var loockStatus = $("#hdfLoockStatus").val();

            if (loockStatus == 0) {
                $("#body-frame").removeClass();
                $("#body-frame").addClass("hold-transition skin-blue sidebar-mini");

                $('#lockscreen').hide();
                $('#wrapper').show();
            } else {
                $("#body-frame").removeClass();
                $("#body-frame").addClass("hold-transition lockscreen");

                $('#lockscreen').show();
                $('#wrapper').hide();
                $('#txtPassWord').focus();
            }
        });
    </script>

